{
  "include": [
    "src/**/*.py"
  ],
  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    "**/build"
  ],
  "ignore": [
    "**/.venv"
  ],
  "defineConstant": {
    "DEBUG": true
  },
  "venvPath": ".",
  "venv": ".venv",
  "reportMissingImports": true,
  "reportMissingTypeStubs": false,
  "pythonVersion": "3.11",
  "pythonPlatform": "Windows",
  "typeCheckingMode": "basic"
}
